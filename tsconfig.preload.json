{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./dist-electron/preload",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["node"]
  },
  "include": ["src/preload/**/*"],
  "exclude": ["node_modules", "dist-electron"]
}
